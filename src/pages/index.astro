---
import Base from "$layout/Base.astro";
import Transition from "src/components/Transition.astro";
---

<Base title="m" pageOrder={0}>
<div class="flex h-full">
	<svg viewBox="0 0 256 256">
		<g transform="translate(128 128)">
			<circle r="64" />
		</g>
	</svg>
</div>
</Base>

<style lang="scss">
	.flex {
		:global(> div) {
			flex-grow: 1;
		}
	}

	svg {
		width: 100%;
		height: 100%;
	}
</style>